{
    "cSpell.words": [
        "Morshead",
        "pytest",
        "runserver"
    ],

    "python.defaultInterpreterPath": "${workspaceRoot}/.venv/scripts/bin/python",
    // "python.defaultInterpreterPath": "${workspaceRoot}/src/py/reactpy/.venv/reactpy/bin/python",

    "python.analysis.diagnosticMode": "workspace",
